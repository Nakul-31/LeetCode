class Solution(object):
    def doesAliceWin(self, s):
        """
        :type s: str
        :rtype: bool
        """
        vowels = set("aeiou")
        for ch in s:
            if ch in vowels:
                return True  # Alice can always win if there’s at least one vowel
        return False  # No vowels → Alice cannot move → she loses
