class Solution(object):
    def getNoZeroIntegers(self, n):
        """
        :type n: int
        :rtype: List[int]
        """
        # Helper function to check if a number contains zero
        def is_no_zero(num):
            return '0' not in str(num)
        
        # Try all possible splits
        for a in range(1, n):
            b = n - a
            if is_no_zero(a) and is_no_zero(b):
                return [a, b]
